{
  "expo": {
    "name": "CarBrandsApp",
    "slug": "CarBrandsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/car.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/car.png",
      "resizeMode": "contain",
      "backgroundColor": "#0744DE"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/car.png",
        "backgroundColor": "#0744DE"
      }
    },
    "web": {
      "favicon": "./assets/car.png"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
